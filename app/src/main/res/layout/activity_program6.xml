<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".program.Program5">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout style="@style/button_wrapper_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="25dp"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:paddingLeft="@dimen/qmui_content_spacing_horizontal"
            android:paddingRight="@dimen/qmui_content_spacing_horizontal">

            <!--    1-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:orientation="vertical">
                <TextView
                    style="@style/QDCommonTitle"
                    android:text="@string/program6_title_1" />

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_1_start"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="@string/program_start"
                    app:qmui_isRadiusAdjustBounds="true"/>

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_1_stop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="@string/program_stop"
                    app:qmui_isRadiusAdjustBounds="true"/>

            </LinearLayout>
            <!--    2-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:orientation="vertical">
                <TextView
                    style="@style/QDCommonTitle"
                    android:text="@string/program6_title_2" />

                <TextView
                    style="@style/QDCommonTitle"
                    android:id="@+id/program6_2_text"
                    android:text="" />

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_2_start"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="@string/program_start"
                    app:qmui_isRadiusAdjustBounds="true"/>

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_2_stop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="@string/program_stop"
                    app:qmui_isRadiusAdjustBounds="true"/>

            </LinearLayout>
            <!--    3-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:orientation="vertical">
                <TextView
                    style="@style/QDCommonTitle"
                    android:text="@string/program6_title_3" />

                <TextView
                    style="@style/QDCommonTitle"
                    android:id="@+id/program6_3_text"
                    android:text="" />

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_3_bind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="服务绑定"
                    app:qmui_isRadiusAdjustBounds="true"/>

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_3_unbind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="取消绑定"
                    app:qmui_isRadiusAdjustBounds="true"/>

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_3_compute"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="求和"
                    app:qmui_isRadiusAdjustBounds="true"/>

            </LinearLayout>
            <!--    4-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"
                android:orientation="vertical">
                <TextView
                    style="@style/QDCommonTitle"
                    android:text="@string/program6_title_4" />

                <TextView
                    style="@style/QDCommonTitle"
                    android:id="@+id/program6_4_text"
                    android:text="" />

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_4_bind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="服务绑定"
                    app:qmui_isRadiusAdjustBounds="true"/>

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_4_unbind"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="取消绑定"
                    app:qmui_isRadiusAdjustBounds="true"/>

                <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/program6_4_compute"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginRight="?attr/qmui_content_spacing_horizontal"
                    android:layout_marginTop="20dp"
                    android:gravity="center"
                    android:paddingBottom="10dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="16dp"
                    android:paddingTop="10dp"
                    android:text="求和"
                    app:qmui_isRadiusAdjustBounds="true"/>

            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>